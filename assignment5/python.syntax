"#.*(?:$|\n)": comment
"(\w+)(?=\(.*\))": function
"(?:from\s+[a-zA-Z_\.]+\s+)?import\s[a-zA-Z_\.]+(?:\sas\s+[a-zA-Z_\.]+)?": import
"(?:\s)((?:None)|(?:False)|(?:True))(?:\s)": special
"(?:\s)((?:try)|(?:except))(?:\s)": try_except
"(?:\s)((?:if)|(?:elif)|(?:else))(?:\s)": if
"(?:\s)(for)(?:\s)": for
"(?:\s)(while)(?:\s)": while
"(?s:[\"\']{3}.*?[\"\']{3})": string_block
"[\"\'].*[\"\']": string
